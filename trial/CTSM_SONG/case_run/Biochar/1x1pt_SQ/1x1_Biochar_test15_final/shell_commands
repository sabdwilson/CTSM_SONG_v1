# shell commands to execute xmlchange --force commands written by PTCLMmkdata:
# /var/spool/slurm/d/job8557614/slurm_script --ctsm_root /home/u8/sdwilson/CTSM_SONG -s SQ -d /xdisk/chopinsong/chopinsong/CTSM_inputdata --map_gdate 231113 --crop --soilgrid --mydatadir=/groups/chopinsong/CTSM_sabrina/PTCLM/mydatafile/SQ --clmnmlusecase=1850-2000_transient 
./xmlchange --force CLM_USRDAT_NAME=1x1pt_SQ
./xmlchange --force DATM_CLMNCEP_YR_START=1980
./xmlchange --force DATM_CLMNCEP_YR_END=2012
# Comment this out if NINST_LND is greater than 1 (see: http://bugs.cgd.ucar.edu/show_bug.cgi?id=2521)
./xmlchange --force MPILIB=mpi-serial
./xmlchange --force ATM_DOMAIN_PATH=/groups/chopinsong/CTSM_sabrina/PTCLM/mydatafile/SQ/1x1pt_SQ
./xmlchange --force LND_DOMAIN_PATH=/groups/chopinsong/CTSM_sabrina/PTCLM/mydatafile/SQ/1x1pt_SQ
./xmlchange --force ATM_DOMAIN_FILE=domain.lnd.1x1pt_SQ_navy.231113.nc
./xmlchange --force LND_DOMAIN_FILE=domain.lnd.1x1pt_SQ_navy.231113.nc
./xmlchange --force --append CLM_BLDNML_OPTS='-mask navy -crop'
./xmlchange --force CALENDAR=GREGORIAN
./xmlchange --force DOUT_S=FALSE
./xmlchange --force ATM_NCPL=48
./xmlchange --force RUN_STARTDATE=1980-01-01
./xmlchange --force DATM_CLMNCEP_YR_ALIGN=1980
./xmlchange --force DIN_LOC_ROOT=/xdisk/chopinsong/chopinsong/CTSM_inputdata
./xmlchange --force DIN_LOC_ROOT_CLMFORC=/groups/chopinsong/CTSM_sabrina/PTCLM/mydatafile/SQ
