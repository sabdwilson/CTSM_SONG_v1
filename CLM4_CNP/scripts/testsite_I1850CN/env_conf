
setenv RUN_TYPE  startup     # startup,hybrid,branch
setenv RUN_STARTDATE  0001-01-01     # Run start date (yyyy-mm-dd). Only used for startup or hybrid runs
setenv RUN_REFCASE  case.std     # Reference case for hybrid or branch runs
setenv RUN_REFDATE  0001-01-01     # Reference date for hybrid or branch runs (yyyy-mm-dd)
setenv RUN_REFTOD  00000     # Reference time of day (seconds) for hybrid or branch runs (sssss)
setenv BRNCH_RETAIN_CASENAME  FALSE     # TRUE,FALSE
setenv GET_REFCASE  FALSE     # TRUE,FALSE
setenv USE_MPISERIAL  FALSE     # TRUE,FALSE
setenv MPISERIAL_SUPPORT  TRUE     # TRUE,FALSE
setenv NCPL_BASE_PERIOD  day     # hour,day,year,decade
setenv ATM_NCPL  48     # number of atm coupling intervals per NCPL_BASE_PERIOD (integer)
setenv LND_NCPL  $ATM_NCPL     # number of land coupling intervals per NCPL_BASE_PERIOD (integer)
setenv ICE_NCPL  $ATM_NCPL     # number of ice coupling intervals per NCPL_BASE_PERIOD (integer)
setenv OCN_NCPL  1     # number of ocn coupling intervals per NCPL_BASE_PERIOD (integer)
setenv GLC_NCPL  1     # number of glc coupling intervals per NCPL_BASE_PERIOD (integer)
setenv GLC_NEC  0     # 0,1,3,5,10


setenv ATM_DOMAIN_FILE  domain.lnd.${CLM_USRDAT_NAME}_navy.nc     # atm domain file
setenv ATM_DOMAIN_PATH  $DIN_LOC_ROOT/share/domains/domain.clm     # path of atm domain file
setenv LND_DOMAIN_FILE  domain.lnd.${CLM_USRDAT_NAME}_navy.nc     # lnd domain file
setenv LND_DOMAIN_PATH  $DIN_LOC_ROOT/share/domains/domain.clm     # path of lnd domain file
setenv ICE_DOMAIN_FILE  UNSET     # ice domain file
setenv ICE_DOMAIN_PATH  $DIN_LOC_ROOT/share/domains     # path of ice domain file
setenv ICE_DOMAIN_PRIMEFACS       # prime factors of ice domain grid size
setenv OCN_DOMAIN_FILE  UNSET     # ocn domain file
setenv OCN_DOMAIN_PATH  $DIN_LOC_ROOT/share/domains     # path of ocn domain file
setenv MAP_A2OF_FILE  idmap     # atm to ocn flux mapping file for fluxes
setenv MAP_A2OS_FILE  idmap     # atm to ocn state mapping file for states
setenv MAP_O2AF_FILE  idmap     # ocn to atm mapping file for fluxes
setenv MAP_O2AS_FILE  idmap     # ocn to atm mapping file for states
setenv MAP_A2LF_FILE  idmap     # atm to land mapping file for fluxes
setenv MAP_A2LS_FILE  idmap     # atm to land mapping file for states
setenv MAP_L2AF_FILE  idmap     # land to atm mapping file for fluxes
setenv MAP_L2AS_FILE  idmap     # land to atm mapping file for states
setenv MAP_R2O_FILE_R05  idmap     # runoff (.5 degree) to ocn mapping file
setenv MAP_R2O_FILE_R19  idmap     # runoff (19 basin) to ocn mapping file
setenv MAP_R2O_FILE_RX1  idmap     # runoff (1 degree) to ocn mapping file
setenv MAP_A2O_PATH  $DIN_LOC_ROOT/cpl/cpl6     # path of mapping files
setenv MAP_O2A_PATH  $DIN_LOC_ROOT/cpl/cpl6     # path of mapping files
setenv MAP_L2A_PATH  $DIN_LOC_ROOT/cpl/cpl6     # path of mapping files
setenv MAP_A2L_PATH  $DIN_LOC_ROOT/cpl/cpl6     # path of mapping files
setenv MAP_R2O_PATH  $DIN_LOC_ROOT/cpl/cpl6     # path of mapping files

setenv CPL_EPBAL  off     # off,ocn
setenv CPL_ALBAV  false     # true,false
setenv CCSM_BGC  none     # none,CO2A,CO2B,CO2C,CO2_DMSA
setenv CCSM_VOC  FALSE     # TRUE,FALSE
setenv CCSM_CO2_PPMV  284.7     # CO2 ppmv

setenv SSTICE_STREAM  UNSET     # Sets sst/ice_cov stream name for pres runs, only used in F,A compset
setenv SSTICE_DATA_FILENAME  UNSET     # Sets sst/ice_cov data filename for pres runs, only used in F,A compset
setenv SSTICE_GRID_FILENAME  UNSET     # Sets sst/ice_cov grid filename for pres runs, only used in F,A compset
setenv SSTICE_YEAR_ALIGN  -999     # Sets model year align of sst/ice_cov for pres runs, only used in F,A compset
setenv SSTICE_YEAR_START  -999     # Sets year start of sst/ice_cov for pres runs, only used in F,A compset
setenv SSTICE_YEAR_END  -999     # Sets year end of sst/ice_cov for pres runs, only used in F,A compset

setenv DATM_MODE  CLM1PT     # CORE2_NYF,CORE2_IAF,TN460,CLM_QIAN,CLM_CRUNCEP,CLM1PT,CPLHIST3HrWx
setenv DATM_PRESAERO  clim_1850     # none,clim_1850,clim_2000,trans_1850-2000,rcp2.6,rcp4.5,rcp6.0,rcp8.5,pt1_pt1
setenv DATM_CLMNCEP_YR_ALIGN  1     # year align (only used for CLM_QIAN mode)
setenv DATM_CLMNCEP_YR_START  1948     # starting year to loop data over (only used for CLM_QIAN mode)
setenv DATM_CLMNCEP_YR_END  1972     # ending year to loop data over (only used for CLM_QIAN mode)
setenv DATM_CPL_CASE  UNSET     # case name for coupler history data mode (only used for CPLHIST3HrWx mode)
setenv DATM_CPL_YR_ALIGN  -999     # year align (only used for CPLHIST3HrWx mode)
setenv DATM_CPL_YR_START  -999     # starting year to loop data over (only used for CPLHIST3HrWx mode)
setenv DATM_CPL_YR_END  -999     # ending year to loop data over (only used for CPLHIST3HrWx mode)

setenv CLM_CONFIG_OPTS  "-bgc cn"     # CLM configure options
setenv CLM_BLDNML_OPTS       # CLM build-namelist options
setenv CLM_NML_USE_CASE  1850_control     # CLM namelist use_case
setenv CLM_NAMELIST_OPTS       # CLM specific namelist settings
setenv CLM_CO2_TYPE  constant     # constant,diagnostic,prognostic
setenv CLM_FORCE_COLDSTART  off     # on,off
setenv CLM_USRDAT_NAME  UNSET     # My Data ID
